{
  "name": "ptoid",
  "version": "1.0.4",
  "description": "PTOID: Local-first document automation system with full offline AI processing (speech-to-text, OCR, document parsing, receipt extraction, and local NLP processing). 100% open-source and GDPR-compliant.",
  "author": "Levin Pflicht",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/levinpflicht/PTOID"
  },
  "homepage": "https://github.com/levinpflicht/PTOID",
  "bugs": {
    "url": "https://github.com/levinpflicht/PTOID/issues"
  },
  "keywords": [
    "local-ai",
    "document-automation",
    "OCR",
    "speech-to-text",
    "whisper",
    "tesseract",
    "invoice-processing",
    "react",
    "electron",
    "open-source"
  ],
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "com.ptoid.app",
    "productName": "PTOID",
    "win": {
      "target": "nsis"
    }
  },
  "devDependencies": {
    "electron": "^28.3.3",
    "electron-builder": "^24.6.0"
  },
  "dependencies": {
    "axios": "^1.3.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
